CREATE TABLE FavoriteComics (
    id INTEGER NOT NULL PRIMARY KEY,
    title TEXT NOT NULL,
    description TEXT NOT NULL,
    thumbnail TEXT NOT NULL
);

getFavortieComics:
SELECT * FROM FavoriteComics;

saveFavorite:
INSERT OR REPLACE INTO FavoriteComics (id, title, description, thumbnail) VALUES (?,?,?, ?);

removeFavorite:
DELETE FROM FavoriteComics WHERE id = ?;